#
#REMAP=M6   modalgroup=6  prolog=change_prolog   ngc=rack_change  epilog=change_epilog


[EMCMOT]
EMCMOT = motmod
COMM_TIMEOUT = 1.0
COMM_WAIT = 0.010
SERVO_PERIOD = 1000000
#SERVO_PERIOD = 700000

[TOOL_SLOT_DELTA]
X = 0
Y = -118.6  
Z = 0

# this is not named TOOL_CHANGE_POSITION on purpose - canon uses it and will
# interfere
[CHANGE_POSITION]
X = 3057
Y = 882.6  
Z = 0

[DWELL]
LOCK_TIME = 0.5
MYSTERY = 0
NEW_TOOL = 1.0
POST_UNLOCK = 0.5

[HAL]
HALUI = halui
HALFILE = justinbieber.hal
HALFILE = io.hal
HALFILE = custom.hal
HALFILE = racktoolchange.hal
POSTGUI_HALFILE = postgui_call_list.hal
POSTGUI_HALFILE = pyvcp_options.hal
SHUTDOWN = shutdown.hal

[HALUI]

[TRAJ]
AXES = 3
COORDINATES = X Y Z
LINEAR_UNITS = mm
ANGULAR_UNITS = degree
CYCLE_TIME = 0.010
DEFAULT_VELOCITY = 100
MAX_LINEAR_VELOCITY = 5000

# DEFAULT_ACCELERATION =  1000.0
# MAX_ACCELERATION =      10000.0

